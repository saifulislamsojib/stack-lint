{
  "name": "@stack-lint/react",
  "version": "0.0.9",
  "description": "stack lint eslint configs for react",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "default": "./index.js",
      "types": "./index.d.ts"
    },
    "./compiler": {
      "default": "./reactCompilerConfig.js",
      "types": "./reactCompilerConfig.d.ts"
    },
    "./next": {
      "default": "./next.js",
      "types": "./next.d.ts"
    }
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "stack-lint",
    "react-eslint",
    "stack-lint-react",
    "@stack-lint/react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/saifulislamsojib/eslint-config"
  },
  "author": "https://github.com/saifulislamsojib",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "dependencies": {
    "globals": "^16.3.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-prefer-function-component": "^4.0.1"
  },
  "peerDependencies": {
    "@stack-lint/base": ">=0.0.8",
    "eslint": ">=9",
    "prettier": ">=3",
    "react": ">=19",
    "@next/eslint-plugin-next": ">=15.1"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  }
}
